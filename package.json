{
  "name": "@testjg/mtmobile-lib",
  "version": "0.3.2",
  "description": "Set of utils used by MantTestMobile",
  "keywords": [],
  "author": "isierra <isierra@jgingenieros.es>",
  "repository": {
    "type": "git",
    "url": "github:TestJG/mtmobile-lib"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "node": ">=14.X"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts src spec",
    "lint:fix": "eslint --fix --ext .ts src spec",
    "clean": "rimraf dist && rimraf compiled && rimraf node_modules/.cache",
    "types": "tsc -p tsconfig.lib.json --emitDeclarationOnly",
    "docs": "typedoc --out compiled/docs src/index.ts",
    "build": "rollup -c",
    "dev": "npm run test:watch",
    "test": "jest --all",
    "test:watch": "jest --watch --onlyChanged",
    "test:prod": "npm run test -- --coverage --no-cache",
    "release": "bumpp --commit --push --tag"
  },
  "devDependencies": {
    "@angular/core": "~15.1.5",
    "@ngrx/store": "^15.4.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bumpp": "^8.2.1",
    "colors": "^1.4.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.2",
    "js-csp": "github:TestJG/js-csp",
    "prettier": "^2.8.8",
    "rimraf": "^2.7.1",
    "rollup": "^3.27.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5",
    "zone.js": "^0.12.0"
  },
  "peerDependencies": {
    "js-csp": "TestJG/js-csp",
    "rxjs": "^7.8.1"
  }
}
